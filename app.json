{
  "expo": {
    "name": "Simple 5Ã—5",
    "slug": "simple-5x5",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0f63f9"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bibire.simple5x5",
      "buildNumber": "2",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#0f63f9"
      },
      "package": "com.bibire.simple5x5",
      "versionCode": 2,
      "permissions": [
        "VIBRATE",
        "POST_NOTIFICATIONS",
        "USE_EXACT_ALARM",
        "SCHEDULE_EXACT_ALARM",
        "WAKE_LOCK"
      ]
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#0f63f9"
    },
    "notification": {
      "icon": "./assets/images/notification-icon.png",
      "color": "#0f63f9",
      "androidMode": "default",
      "androidCollapsedTitle": "Rest Timer"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#0f63f9",
          "sounds": ["./assets/set_bell.wav"]
        }
      ]
    ],
    "scheme": "simple5x5",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1b20edd1-14f0-472d-a71d-505ce99ef2d5"
      }
    }
  }
}
