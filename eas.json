{
  "cli": {
    "version": ">= 16.19.3",
    "appVersionSource": "remote",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production"
    },
    "production-apk": {
      "android": {
        "buildType": "apk"
      },
      "developmentClient": false,
      "distribution": "internal"
    }
  },
  "submit": {
    "production": {}
  }
}
